page {
    background-color: #212121;
}


/* 字体文件 */

@font-face {
    font-family: youth;
    src: url(https://jts.egg404.cn/b.ttf);
}

.main {
    margin: auto 10px;
    /* width: 96vw; */
    height: 95vh;
    padding: 30rpx;
}

.main .move-area {
    margin: auto 0;
    /* width: 100vw; */
    /* height: 95vh; */
}

.move-area .move-view {
    width: 2479px;
    height: 3508px;
    /* color: rgb(164, 41, 41); */
}

.move-area .move-view .content {
    position: relative;
    width: 2479px;
    height: 3508px;
    border-radius: 5px;
    overflow: hidden;
    background-size: cover;
    box-shadow: 0 1px 5px rgba(255, 255, 255, 0.45);
    /* https://tuchuangs.com/imgs/2022/09/17/26d339f3c615fa3a.png */
    /* https://tuchuangs.com/imgs/2022/09/17/4d38425d8d5948ba.png */
    /* /* background-image: url(https://tuchuangs.com/imgs/2022/09/17/4d38425d8d5948ba.png) */
    background-image: url(https://tuchuangs.com/imgs/2022/09/17/26d339f3c615fa3a.png)
}

.move-area .move-view .content .article {
    width: 100%;
    height: 100%;
    z-index: 99;
    /* background-color: rgba(18, 216, 78, 0.47); */
    box-sizing: border-box;
    font-family: youth;
}

.article {
    font-size: 62px;
    padding-left: 40px;
    padding-top: 59px;
    -webkit-text-stroke: 1rpx #000000;
}

.worldsStyle {
    display: inline-block;
    height: 109px;
    width: 109px;
    margin-top: 31px;
    text-align: center;
    line-height: 109px;
    /* background-color: rgba(219, 23, 23, 0.278); */
}

.canvas {
    width: 2479px;
    height: 3508px;
    /* margin-top: -3500px; */
}

button {
    z-index: 999;
}